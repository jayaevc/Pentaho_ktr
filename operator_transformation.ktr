    <connection>ruby-prod</connection>
    <sql>SELECT
  id as id_ruby
, uuid_in(overlay(overlay(md5(random()::text || ':' || random()::text) placing '4' from 13) placing to_hex(floor(random()*(11-8+1) + 8)::int)::text from 17)::cstring) as id
, name
, NOW() as created_at
, NOW() as updated_at
, emails
, 'FALSE' as is_deleted
FROM operators</sql>
